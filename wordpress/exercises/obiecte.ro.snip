§desc: caracter duplicate
§id: 3
§solution:
// citim textul de la utilizator 
let text = prompt('Text=');

// cream un obiect ce va stoca numarul de aparitii a fiecarei litere
let aparitii = {}

// parcurgem textul caracter cu caracter
for (let i = 0; i < text.length; i++) {
    // cautam litera text[i] in obiectul de aparitii
    if (!(text[i] in aparitii)) {
        // daca nu exista atunci o introduce si mentionam ca este prima aparitie
        aparitii[text[i]] = 1;
    } else {
        // daca deja am intalnit litera atunci afisam ca fiind duplicata
        if (aparitii[text[i]] == 1)
            console.log('Caracter duplicat', text[i]);
            
        // incrementam numarul de aparitii ca urmatoarele intalniri ale literei sa nu mai genereze mesake
        aparitii[text[i]] += 1;
    }
}
