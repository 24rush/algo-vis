                {'nume': 'Mary', 'scor': 132},
                {'nume': 'Johnny', 'scor': 133323},];

// citim numele de la utilizator
let nume = prompt('Nume=');

// parcurgem vectorul si afisam scorul jucatorului daca il gasim
let gasit = false;
for (let i = 0; i < jucatori.length; i++)
    if (jucatori[i].nume == nume) {
        console.log('Scorul lui', nume, 'este', jucatori[i].scor)
        gasit = true;        
    }

if (!gasit) {
    console.log('Nu exista acest jucator');
}

